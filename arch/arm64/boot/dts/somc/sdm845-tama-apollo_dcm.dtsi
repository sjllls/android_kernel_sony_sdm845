#include "sdm845-tama-apollo_common.dtsi"

&soc {
	/* SPI: QUP4 */
	spi@890000 {
		pinctrl-0 = <&sdm_gpio_89 &sdm_gpio_90 &sdm_gpio_91 &sdm_gpio_92>;
		pinctrl-1 = <&sdm_gpio_89 &sdm_gpio_90 &sdm_gpio_91 &sdm_gpio_92>;
		spi-max-frequency = <12000000>;
		status = "okay";

		onesegspi@0 {
			compatible = "sony,vj190-spi";
			reg = <0>;
			spi-max-frequency = <12000000>;
			spi-mode = <0>;
		};
	};

	oneseg_tuner: vj190 {
		compatible = "sony,vj190";
		interrupt-parent = <&tlmm>;
		interrupts = <26 0>;
		gpios = <&pm8998_gpios 16 0>,	/* DTV_ACTIVE  */
				<&tlmm 25 0>,	/* DTV_RESET_N */
				<&tlmm 26 0>;	/* DTV_INT     */
	};
};

&somc_pinctrl {
	pinctrl-2 = <&sdm_gpio_12 &sdm_gpio_25 &sdm_gpio_26 &sdm_gpio_63 &sdm_gpio_89
		     &sdm_gpio_90 &sdm_gpio_91 &sdm_gpio_92 &sdm_gpio_105 &sdm_gpio_106
		     &sdm_gpio_107 &sdm_gpio_108 &sdm_gpio_129>;
};

&somc_pinctrl_pmic {
	pinctrl-2 = <&pm8998_gpio_16>;
};

/* GPIO_16: DTV_PWR_EN */
&pm8998_gpio_16 {
	pins = "gpio16";
	/delete-property/ bias-high-impedance;
	function = "normal";
	output-low;
	drive-push-pull;
	qcom,drive-strength = <3>;
	power-source = <0>;
};

/* GPIO_12: NC */
&sdm_gpio_12 {
	mux {
		pins = "gpio12";
		function = "gpio";
	};

	config {
		pins = "gpio12";
		drive-strength = <2>;
		/delete-property/ bias-disable;
		bias-pull-down;
		/delete-property/ output-low;
		input-enable;
	};
};

