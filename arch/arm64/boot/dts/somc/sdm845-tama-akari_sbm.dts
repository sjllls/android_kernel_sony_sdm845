/dts-v1/;

#include "../qcom/sdm845.dtsi"
#include "../qcom/sdm845-sde-display.dtsi"
#include "../qcom/sdm845-mtp.dtsi"
#include "sdm845-tama-akari_sbm.dtsi"

/ {
	model = "Sony Mobile Communications. Akari-SBM(SDM845 v1)";
	compatible = "somc,akari-sbm", "qcom,sdm845", "qcom,mtp";
	qcom,board-id = <8 0>;
};

&android_q_fstab {
	carrier {
		compatible = "android,vendor";
		dev = "/dev/block/platform/soc/1d84000.ufshc/by-name/carrier";
		type = "ext4";
		mnt_flags = "rw,barrier=1,noatime";
		fsmgr_flags = "wait,nofail";
		status = "ok";
	};
};